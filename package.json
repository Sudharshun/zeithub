{
  "name": "konta",
  "version": "0.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "9.11.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node ./src/server/index.ts",
    "dev": "nodemon",
    "precommit": "pretty-quick --staged",
    "tsc": "tsc --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "0.11.2",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "6.0.0",
    "express": "~4.16.0",
    "express-history-api-fallback": "^2.2.1",
    "html2canvas": "0.5.0-beta4",
    "http-errors": "1.7.0",
    "jsonwebtoken": "8.3.0",
    "jspdf": "1.4.1",
    "moment": "2.22.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "mysql2": "^1.6.1",
    "puppeteer": "^1.8.0",
    "slug": "^0.9.1",
    "vue": "2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "1.3.7",
    "assert": "^1.4.1",
    "cpx": "^1.5.0",
    "css-loader": "1.0.0",
    "eslint": "5.5.0",
    "expect": "23.5.0",
    "hard-source-webpack-plugin": "0.12.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "style-loader": "0.23.0",
    "supertest": "3.3.0",
    "ts-loader": "5.0.0",
    "ts-node": "7.0.1",
    "typescript": "3.0.3",
    "uglifyjs-webpack-plugin": "1.3.0",
    "vue-loader": "15.4.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-middleware": "3.2.0",
    "webpack-merge": "4.1.4"
  }
}
