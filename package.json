{
  "name": "konta",
  "version": "0.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "9.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node ./src/server/index.ts",
    "dev": "nodemon",
    "precommit": "pretty-quick --staged",
    "tsc": "tsc --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "^0.11.1",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "6.0.0",
    "express": "~4.16.0",
    "express-history-api-fallback": "^2.2.1",
    "html2canvas": "0.5.0-beta4",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "8.3.0",
    "jspdf": "1.4.1",
    "moment": "2.22.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "slug": "^0.9.1",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "1.3.7",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "5.0.1",
    "hard-source-webpack-plugin": "0.8.1",
    "husky": "^0.14.3",
    "nodemon": "1.17.5",
    "prettier": "1.13.7",
    "pretty-quick": "^1.6.0",
    "style-loader": "^0.21.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "vue-loader": "15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.13.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-merge": "4.1.3"
  }
}
