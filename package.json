{
  "name": "konta",
  "version": "0.2.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "12.10.0",
    "npm": "6.10.3"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' ./src/server/index.ts",
    "dev": "nodemon",
    "tsc": "tsc --module \"commonjs\" --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build": "npm run webpack:prod && npm run tsc",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "bcrypt": "3.0.7",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.4",
    "db-migrate": "0.11.6",
    "db-migrate-mysql": "^2.1.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "^3.21.2",
    "http-errors": "1.7.3",
    "jsonwebtoken": "8.5.1",
    "luxon": "^1.21.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.0.2",
    "puppeteer-core": "chrome-72",
    "slugify": "^1.3.6",
    "vue": "2.6.11",
    "vue-class-component": "^7.1.0",
    "vue-imask": "5.2.1",
    "vue-property-decorator": "8.3.0",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuetify": "^2.1.15"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/multer": "1.3.10",
    "assert": "^2.0.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cpx": "^1.5.0",
    "css-loader": "3.4.0",
    "eslint": "6.7.2",
    "expect": "24.9.0",
    "fibers": "^4.0.2",
    "husky": "3.1.0",
    "mocha": "^6.2.2",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "1.0.2",
    "supertest": "4.0.2",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "typescript": "3.7.3",
    "vue-loader": "15.8.3",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.4",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-merge": "4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
