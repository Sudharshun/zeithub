{
  "name": "konta",
  "version": "0.2.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "11.4.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' ./src/server/index.ts",
    "dev": "nodemon",
    "tsc": "tsc --module \"commonjs\" --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build": "npm run webpack:prod && npm run tsc",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "0.11.6",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "^3.15.0",
    "http-errors": "1.7.3",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^1.6.5",
    "puppeteer": "1.19.0",
    "slugify": "^1.3.4",
    "vue": "2.6.10",
    "vue-class-component": "^7.0.1",
    "vue-imask": "5.1.9",
    "vue-property-decorator": "8.2.1",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.10"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "4.17.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "1.3.7",
    "assert": "^2.0.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cpx": "^1.5.0",
    "css-loader": "3.1.0",
    "eslint": "6.1.0",
    "expect": "24.8.0",
    "husky": "3.0.2",
    "mocha": "^6.2.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.10.0",
    "style-loader": "0.23.1",
    "supertest": "4.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "vue-loader": "15.7.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.39.1",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-middleware": "3.7.0",
    "webpack-merge": "4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
