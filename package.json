{
  "name": "konta",
  "version": "0.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "9.11.1",
    "npm": "6.0.0"
  },
  "scripts": {
    "start": "ts-node ./src/server/index.ts",
    "dev": "nodemon ./bin/www",
    "tsc": "tsc",
    "lint": "eslint src/",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js"
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "^0.11.1",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "express-history-api-fallback": "^2.2.1",
    "html2canvas": "0.5.0-beta4",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.2.1",
    "jspdf": "^1.3.5",
    "moment": "^2.22.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "slug": "^0.9.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "2.0.15",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
