{
  "name": "konta",
  "version": "0.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "9.11.1"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www --watch src/",
    "lint": "eslint src/",
    "db:up": "db-migrate up --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "heroku-postbuild": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "bcrypt": "^2.0.0",
    "cheerio": "^1.0.0-rc.2",
