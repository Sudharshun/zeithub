    "db-migrate": "^0.10.7",
    "db-migrate-mysql": "^1.1.10",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "express-history-api-fallback": "^2.2.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "slug": "^0.9.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
