{
  "name": "konta",
  "version": "0.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "11.4.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' ./src/server/index.ts",
    "dev": "nodemon",
    "tsc": "tsc --module \"commonjs\" --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "bcrypt": "3.0.3",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "db-migrate": "0.11.4",
    "db-migrate-mysql": "^1.1.10",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "^3.15.0",
    "http-errors": "1.7.1",
    "jsonwebtoken": "8.4.0",
    "moment": "2.23.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^1.6.4",
    "puppeteer": "1.11.0",
    "slugify": "^1.3.4",
    "vue": "2.5.21",
    "vue-class-component": "^6.3.2",
    "vue-imask": "4.1.4",
    "vue-property-decorator": "7.2.0",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "1.3.7",
    "assert": "^1.4.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cpx": "^1.5.0",
    "css-loader": "2.1.0",
    "eslint": "5.11.1",
    "expect": "23.6.0",
    "husky": "1.3.1",
    "mocha": "^5.2.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "pretty-quick": "^1.8.0",
    "style-loader": "0.23.1",
    "supertest": "3.3.0",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "typescript": "3.2.2",
    "vue-loader": "15.4.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-middleware": "3.4.0",
    "webpack-merge": "4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
